{
  "name": "reserve-components",
  "version": "1.0.4",
  "description": "The building-blocks for ReServe.",
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch",
    "test:coverage": "nyc ava",
    "test:report": "nyc report --reporter=html",
    "build:clean": "rm -rf lib && mkdir lib",
    "prebuild": "npm run build:clean",
    "build": "BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "react": "^0.14.7",
    "rxjs": "^5.0.0-beta.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reservejs/reserve-components"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "source": "src/**/*.js",
    "failFast": true,
    "files": "src/**/*.test.js"
  },
  "keywords": [],
  "author": "Marcus Nielsen <marcus.nielsen82@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.9",
    "nyc": "^6.4.0"
  }
}
